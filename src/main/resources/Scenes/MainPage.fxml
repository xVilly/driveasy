<GridPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.driveasy.Controllers.MainController">
   <columnConstraints>
      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <TabPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #d1bff2;" tabClosingPolicy="UNAVAILABLE" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
        <tabs>
          <Tab text="Browse Cars">
            <content>
              <AnchorPane maxHeight="1.7976931348623157E308" minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" style="-fx-background-color: #d1bff2;">
                     <children>
                        <Label layoutX="286.0" layoutY="177.0" text="Browse" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Your Profile">
               <content>
                  <FlowPane alignment="CENTER">
                     <children>
                        <VBox>
                           <children>
                              <FlowPane alignment="CENTER">
                                 <children>
                                    <Label text="Personal information">
                                       <font>
                                          <Font name="JetBrains Mono ExtraBold" size="18.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </FlowPane>
                              <GridPane alignment="CENTER">
                                <columnConstraints>
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                                </columnConstraints>
                                <rowConstraints>
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                                </rowConstraints>
                                 <children>
                                    <FlowPane alignment="CENTER_RIGHT">
                                       <children>
                                          <Label alignment="CENTER_RIGHT" text="First name:">
                                             <font>
                                                <Font name="JetBrains Mono Medium" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                       <GridPane.margin>
                                          <Insets right="8.0" />
                                       </GridPane.margin>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_RIGHT" GridPane.rowIndex="1">
                                       <children>
                                          <Label alignment="CENTER_RIGHT" text="Last name:">
                                             <font>
                                                <Font name="JetBrains Mono Medium" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                       <GridPane.margin>
                                          <Insets right="8.0" />
                                       </GridPane.margin>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_RIGHT" GridPane.rowIndex="2">
                                       <children>
                                          <Label alignment="CENTER_RIGHT" text="Phone number:">
                                             <font>
                                                <Font name="JetBrains Mono Medium" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                       <GridPane.margin>
                                          <Insets right="8.0" />
                                       </GridPane.margin>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_RIGHT" GridPane.rowIndex="3">
                                       <children>
                                          <Label alignment="CENTER_RIGHT" text="Email:">
                                             <font>
                                                <Font name="JetBrains Mono Medium" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                       <GridPane.margin>
                                          <Insets right="8.0" />
                                       </GridPane.margin>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_RIGHT" GridPane.rowIndex="4">
                                       <children>
                                          <Label alignment="CENTER_RIGHT" text="Address:">
                                             <font>
                                                <Font name="JetBrains Mono Medium" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                       <GridPane.margin>
                                          <Insets right="8.0" />
                                       </GridPane.margin>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_LEFT" GridPane.columnIndex="1">
                                       <children>
                                          <Label fx:id="profileFirstName" alignment="CENTER_RIGHT" text="unknown" textFill="#795656">
                                             <font>
                                                <Font name="JetBrains Mono Medium Italic" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                       <GridPane.margin>
                                          <Insets />
                                       </GridPane.margin>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="1">
                                       <children>
                                          <Label fx:id="profileLastName" alignment="CENTER_RIGHT" text="unknown" textFill="#795656">
                                             <font>
                                                <Font name="JetBrains Mono Medium Italic" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="2">
                                       <children>
                                          <Label fx:id="profilePhoneNumber" alignment="CENTER_RIGHT" text="unknown" textFill="#795656">
                                             <font>
                                                <Font name="JetBrains Mono Medium Italic" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="3">
                                       <children>
                                          <Label fx:id="profileEmail" alignment="CENTER_RIGHT" text="unknown" textFill="#795656">
                                             <font>
                                                <Font name="JetBrains Mono Medium Italic" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </FlowPane>
                                    <FlowPane alignment="CENTER_LEFT" GridPane.columnIndex="1" GridPane.rowIndex="4">
                                       <children>
                                          <Label fx:id="profileAddress" alignment="CENTER_RIGHT" text="unknown" textFill="#795656">
                                             <font>
                                                <Font name="JetBrains Mono Medium Italic" size="14.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </FlowPane>
                                 </children>
                              </GridPane>
                              <FlowPane alignment="CENTER">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#onEditProfile" style="-fx-background-color: #c6aff0;" text="Edit profile fields" textFill="#ffe4e4">
                                       <font>
                                          <Font name="JetBrains Mono NL ExtraBold" size="12.0" />
                                       </font>
                                       <cursor>
                                          <Cursor fx:constant="HAND" />
                                       </cursor>
                                    </Button>
                                 </children>
                              </FlowPane>
                           </children>
                        </VBox>
                     </children>
                  </FlowPane>
               </content>
          </Tab>
            <Tab text="Your Orders">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="286.0" layoutY="177.0" text="Orders" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
        </tabs>
      </TabPane>
      <BorderPane maxHeight="30.0" prefHeight="30.0" prefWidth="200.0" style="-fx-background-color: #a17ee0;" GridPane.halignment="CENTER" GridPane.hgrow="NEVER" GridPane.rowIndex="1" GridPane.valignment="BOTTOM" GridPane.vgrow="NEVER">
         <left>
            <Label fx:id="nameLabel" prefHeight="17.0" text="Logged in as" BorderPane.alignment="CENTER">
               <font>
                  <Font name="JetBrains Mono Light" size="12.0" />
               </font>
               <BorderPane.margin>
                  <Insets left="5.0" />
               </BorderPane.margin>
            </Label>
         </left>
         <right>
            <Button fx:id="buttonLogOut" mnemonicParsing="false" onAction="#onLogOut" style="-fx-background-color: #7453b0;" text="Log out" textFill="#d3d3d3" BorderPane.alignment="CENTER">
               <font>
                  <Font name="JetBrains Mono ExtraBold" size="12.0" />
               </font>
               <BorderPane.margin>
                  <Insets right="2.0" />
               </BorderPane.margin>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Button>
         </right>
      </BorderPane>
   </children>
</GridPane>
